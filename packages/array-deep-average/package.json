		"test": "test"
	},
	"files": [
		"lib"
	],
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mi-sec/geonet.git"
	},
	"bugs": {
		"url": "https://github.com/mi-sec/geonet/issues"
	},
	"devDependencies": {
		"chai": "^4.2.0",
		"eslint": "^7.18.0",
		"gen-esm-wrapper": "^1.1.1",
		"mocha": "^8.2.1",
		"nyc": "^15.1.0"
	}
